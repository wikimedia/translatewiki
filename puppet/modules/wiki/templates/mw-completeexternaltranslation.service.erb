# This file is managed by puppet
[Unit]
Description=Complete external translation synchronization
Requisite=mysql.service
After=mysql.service

[Service]
User=<%= @maintenance_user %>
ExecStart=/usr/bin/php <%= @production_dir %>/maintenance/run.php <%= @production_dir %>/extensions/Translate/scripts/completeExternalTranslation.php
ProtectHome=read-only
ProtectSystem=full
Nice=5
